<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">

    <context:component-scan base-package="org.hango.cloud.gdashboard.api"/>

    <bean id="cipherKey" class="java.lang.String">
        <constructor-arg type="java.lang.String" value="111111111111111111111111"/>
    </bean>

    <bean id="sessionCipherKey" class="java.lang.String">
        <constructor-arg type="java.lang.String" value="session.key"/>
    </bean>
</beans>